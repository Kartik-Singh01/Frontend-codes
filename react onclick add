import './App.css';
import React, { useState } from 'react';






const App = () => {
  console.log(useState)
  const [count,setCount] = useState(0);
  console.log(count)

  const handleClick = () => {
    setCount(count+1)
  }


  return (
    <div>
      <h1>{count}</h1>
      <button onClick={handleClick}>+</button>
    </div>
  )
}

export default App


